{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"indent": ["error", 4],
		"quotes": [2, "single"],
		"no-console": "off",
		"linebreak-style": ["error", "windows"],
		"no-underscore-dangle": "off",
	}
}
